<?xml version="1.0" encoding="UTF-8"?>
<user-management>
<roles>
    <role id="admin" description="Access to the admin console with view-only priviledges"/>
    <role id="export" description="Export CSV/TXT files"/>
    <role id="upload" description="Upload Mattermark data"/>
    <role id="upload-extended" description="Upload projects ('global DB') and mappings ('Oglivy SFC')."/>
    <role id="user-management" description="Manage users."/>
</roles>
<users>
    <user name="fiadmine" accelerator="FABulous">
        <access id="admin"/>
        <access id="export"/>
    </user>
    <user name="fiadminue">
        <access id="admin"/>
        <access id="export"/>
        <access id="upload"/>
    </user>
    <user name="fiadmin">
        <access id="admin"/>
        <access id="export"/>
        <access id="upload"/>
        <access id="upload-extended"/>
        <access id="user-management"/>
    </user>
</users>
</user-management>